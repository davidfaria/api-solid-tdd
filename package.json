{
  "name": "api",
  "version": "1.0.0",
  "main": "src/server.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && cross-env NODE_ENV=production && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --no-notify --ignore-watch node_modules src/server.ts",
    "start": "cross-env NODE_ENV=production && node dist/server.js",
    "typeorm": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "test": "cross-env NODE_ENV=test jest  --maxWorkers=50%",
    "test:watch": "cross-env NODE_ENV=test jest --maxWorkers=25% --watchAll",
    "test:unit": "cross-env NODE_ENV=test jest --config=jest-unit.config.ts  --maxWorkers=50%",
    "test:unit:watch": "cross-env NODE_ENV=test jest --config=jest-unit.config.ts  --maxWorkers=25% --watchAll",
    "test:integration": "cross-env NODE_ENV=test jest --config=jest-integration.config.ts  --maxWorkers=50%",
    "test:integration:watch": "cross-env NODE_ENV=test jest --config=jest-integration.config.ts  --maxWorkers=25% --watchAll",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint src --max-warnings=0",
    "format": "prettier --write src/**/*.ts{,x}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "yarn lint --fix"
    ],
    "tests/**/*.ts": [
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "amqplib": "^0.7.1",
    "aws-sdk": "^2.907.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^14.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.21.3",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.7",
    "helmet": "^4.6.0",
    "ioredis": "^4.27.2",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.5.2",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.0",
    "pg": "^8.6.0",
    "rate-limiter-flexible": "^2.2.2",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/amqplib": "^0.5.17",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/ejs": "^3.0.6",
    "@types/express": "^4.17.11",
    "@types/handlebars": "^4.1.0",
    "@types/ioredis": "^4.26.3",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mime": "^2.0.3",
    "@types/multer": "^1.4.5",
    "@types/nodemailer": "^6.4.1",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "copyfiles": "^2.4.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "tsyringe": "^4.5.0",
    "typescript": "^4.2.4"
  }
}
